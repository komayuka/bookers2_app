<%= render 'layouts/sidelist', book:@book_new %>
<h2>Users</h2>
<table class="table table-hover">
 <thead>
  <tr>
   <th>image</th>
   <th>name</th>
   <th colspan="3"></th>
  </tr>
  </thead>
  <tbody>
   <% @users.each do |user| %>
  <tr>
   <td><%= link_to user_path(user.id) do %>
        <%= attachment_image_tag user, :profile_image, size: '40x40', class:"attachment user profile_image fallback" %>
   </td>
       <% end %>
   <td>
      <%= user.name %>
   </td>
   <td>
    <%= link_to "show", user %>
   </td>
   <% end %>
  </tr>
 </tbody>
 </table>